{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/notes.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/notes",
      "dest": "/api/notes.js"
    },
    {
      "src": "/api/notes/(.*)",
      "dest": "/api/notes.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
